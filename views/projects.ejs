    <style>
      :root {
    --primary: #00a2ff;
    --secondary: #0d1117;
    --accent: #00ffc6;
    --light: #1c1f26;
    --dark: #e6edf3;
}

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
}

body {
    background-color: #0d1117;
    color: var(--dark);
    line-height: 1.6;
}

header {
    background: linear-gradient(135deg, #0d1117, #161b22);
    color: #fff;
    text-align: center;
    padding: 3rem 1rem;
    position: relative;
    border-bottom: 4px solid var(--accent);
    
}

.back-arrow {
    position: absolute;
    top: 1.5rem;
    left: 1.5rem;
    font-size: 1.8rem;
    color: #fff;
    text-decoration: none;
    transition: color 0.3s ease;
}

.back-arrow:hover {
    color: var(--accent);
}

.header-content h1 {
    font-size: 2.8rem;
    margin-bottom: 0.5rem;
}

.tagline {
    font-size: 1.2rem;
    font-style: italic;
    opacity: 0.9;
    margin-bottom: 1rem;
}

.header-logo {
    height: 120px;
    width: 150px;
    margin-top: 1rem;
    border-radius: 12px;
    box-shadow: 0 4px 10px rgba(0,0,0,0.6);
}

.container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 2rem;
}

.category-tabs {
    display: flex;
    justify-content: center;
    margin-bottom: 2rem;
    flex-wrap: wrap;
}

.tab-btn {
    padding: 0.8rem 1.5rem;
    margin: 0 0.5rem;
    background: var(--light);
    border: none;
    border-radius: 30px;
    cursor: pointer;
    font-weight: 600;
    transition: all 0.3s ease;
    color: var(--dark);
}

.tab-btn.active {
    background: var(--accent);
    color: #0d1117;
}

.tab-btn:hover:not(.active) {
    background: #2a2f3d;
    color: var(--accent);
}

.projects-grid {
    display: grid;
    grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
    gap: 2rem;
}

.project-card {
    background: #161b22;
    border-radius: 8px;
    overflow: hidden;
    box-shadow: 0 5px 15px rgba(0,0,0,0.5);
    transition: transform 0.3s ease;
}

.project-card:hover {
    transform: translateY(-10px);
    box-shadow: 0 8px 20px rgba(0, 255, 200, 0.2);
}

.project-image {
    height: 200px;
}

.project-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.5s ease;
}

.project-imageh {
    height: 190px;
}

.project-imageh img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    transition: transform 0.5s ease;
}

.project-imagez img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    transition: transform 0.5s ease;
}

.project-card:hover .project-image img {
    transform: scale(1.1);
}

.project-info {
    padding: 1.5rem;
}

.project-info h3 {
    margin-bottom: 0.5rem;
    color:#caff0a;
}

.project-info p {
    margin-bottom: 1rem;
    color: #9aa5b1;
}

.project-tags {
    display: flex;
    flex-wrap: wrap;
    gap: 0.5rem;
    margin-bottom: 1rem;
}

.tag {
    background: rgba(0, 255, 200, 0.08);
    padding: 0.3rem 0.8rem;
    border-radius: 20px;
    font-size: 0.8rem;
    color: var(--accent);
}

.project-links {
    display: flex;
    gap: 1rem;
}

.project-links a {
    display: inline-block;
    padding: 0.5rem 1rem;
    border-radius: 4px;
    text-decoration: none;
    font-weight: 600;
    transition: all 0.3s ease;
}

.demo-btn {
    background: linear-gradient(45deg, var(--primary), var(--accent));
    color: #fff;
}

.code-btn {
    background: var(--light);
    color: var(--dark);
}

.project-links a:hover {
    opacity: 0.9;
    transform: translateY(-2px);
}

.category-content {
    display: none;
}

.category-content.active {
    display: block;
    animation: fadeIn 0.5s ease;
}

@keyframes fadeIn {
    from { opacity: 0; }
    to { opacity: 1; }
}

@media (max-width: 768px) {
    .tab-btn {
        margin: 0.3rem;
        padding: 0.6rem 1rem;
        font-size: 0.9rem;
    }
    
    .projects-grid {
        grid-template-columns: 1fr;
    }
}

    </style>
